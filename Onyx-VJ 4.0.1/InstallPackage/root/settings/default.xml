<settings>

	<metadata>
		<version>4.1.0</version>
	</metadata>

	<!-- CORE SETTINGS FOR RENDERING -->
	<core>

		<render>
			<!-- the width / height of bitmaps -->
			<bitmapData>
				<width>320</width>
				<height>240</height>
			</bitmapData>
		</render>
		
		<!-- custom ordering of blendmodes for blendmode controls -->
		<blendModes>
			<normal/>
			<lighten/>
			<darken/>
			<overlay/>
			<hardlight/>
			<multiply/>
			<screen/>
			<add/>
			<subtract/>
			<difference/>
			<invert/>
		</blendModes>

		<cameras>
			<camera>
				<name>DEFAULT</name>
				<width>320</width>
				<height>240</height>
				<frameRate>30</frameRate>
			</camera>
			<!--	ADD MORE CAMERAS IF YOUR CAMERA DOES NOT SUPPORT DEFAULT SIZES
				(IF YOU HAVE AN HD CAPTURE CARD)
			<camera>
				<name>Avermedia Camera</name>
				<width>640</width>
				<height>480</height>
				<frameRate>29.97</frameRate>
			</camera>
			-->
		</cameras>
		<razuna>
      		<login>onyxvj</login>
      		<pwd>null</pwd>
    	</razuna>
    	<videopong>
			<login>batchass</login>
			<pwd>null</pwd>
			<domain>https://www.videopong.net</domain>
		</videopong>
		
	</core>
	
	<!-- UI STATES -->
	<ui>
		
