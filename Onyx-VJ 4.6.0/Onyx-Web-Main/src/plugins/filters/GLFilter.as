package plugins.filters
{
	
	import flash.display.*;
	import flash.events.*;
	import flash.filters.ShaderFilter;
	import flash.geom.Point;
	import flash.system.System;
	import flash.utils.getTimer;
	
	import onyx.core.*;
	import onyx.parameter.*;
	import onyx.plugin.*;
	import onyx.utils.Base64Decoder;
	
	/**
	 * ...
	 * @author lizhi
	 */
	use namespace onyx_ns;
	
	public final class GLFilter extends Filter implements IBitmapFilter {
		
		private var shader:Shader;
		private var loader:Loader;
		private var shaderFilter:ShaderFilter;
		
		private var shaders:Array 	= [];
		private var _type:String	= 'ReliefTunnel';
		private var pbjstrs:Array 	= [
			["zInvert",""],
			["ReliefTunnel",""],
			["Pulse",""],
			["flower",""],
			["Julia", ""],
			["Metablob",""]
			//,["Heart", ""]
		];
		public function GLFilter() 
		{
			parameters.addParameters(
				new ParameterArray('type', 'type', ['zInvert', 'ReliefTunnel', 'Pulse', 'flower', 'Julia', 'Metablob'], _type)//, 'Heart'
			);
			_type		= 'ReliefTunnel';
			
			var lists:Array = [];
			var input:ShaderInput;
			for each(var arr:Array in pbjstrs) {
				lists.push(arr[0]);
				var b64:Base64Decoder  = new Base64Decoder;
				b64.decode(arr[1]);
				var shader:Shader = new Shader(b64.flush());
				if (input == null) input = shader.data.src;
				if (shader.data.src == null) shader.data.src = input;
				shaders[arr[0]] = shader;
			}
			select(type);
		}
		
		
		private function select(name:String):void {
			shader = shaders[name];
			shaderFilter = new ShaderFilter(shader);
		}
		
		public function applyFilter(bitmapData:BitmapData):void {
			
			if (shader) 
			{
				shader.data.time.value[0] = getTimer() / 1000;	
			}
			bitmapData.applyFilter(bitmapData, DISPLAY_RECT, ONYX_POINT_IDENTITY, shaderFilter);
		}
		
		override public function dispose():void {
			
		}		
		
		public function get type():String
		{
			return _type;
		}
		
		public function set type(value:String):void
		{
			_type = value;
			select(_type);
		}
	}
}